<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="techerLanding.css" />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap"
      rel="stylesheet"
    />
    <title>Teacher</title>
  </head>
  <body>
    <div class="main">
      <div class="container">
        <div class="heading">Courses</div>
        <div class="course_container">
          <% for(let i of data) { %>

          <div class="course takequiz" id="<%=i.coursename %>">
            <div class="course_heading"><%=i.coursename %></div>
            <div class="course_description"><%= i.coursedescription %></div>
            <div class="course_passing_marks">
              PASSING MARKS : <%= i.passingmarks %>
            </div>
            <div class="buttons">
              <a class="button_link" href=""><button>Edit</button></a>
              <a class="button_link" href=""><button>Add Question</button></a>
              <a class="button_link" href="/courses/delete/<%=i.coursename%>">
                <button>Delete</button></a
              >
            </div>
          </div>
          <% } %>
        </div>
        <a href="/addCourse"><button>Add Course</button></a>
      </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="teacherLandingpage.js"></script>
  </body>
</html>
